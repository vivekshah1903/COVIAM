/**/
.pst-rel{position:relative;}
a .tooltiptext {
    visibility: hidden;
    width: 120px;
    background-color: black;
    color: #fff;
    text-align: center;
    /*border-radius: 6px;*/
    padding: 5px 0;
    top: 25px;
    left: 6px;
    /* Position the tooltip */
    position: absolute;
    z-index: 1;
}

a:hover .tooltiptext {
    visibility: visible;
}
a .tooltiptext::after {
    content: "";
    position: absolute;
    bottom: 100%;
    left: 14%;
    margin-left: -5px;
    border-width: 5px;
    border-style: solid;
    border-color: transparent transparent #000 transparent;

}

