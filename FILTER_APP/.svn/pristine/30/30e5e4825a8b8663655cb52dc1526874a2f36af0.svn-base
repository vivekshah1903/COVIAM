.udrTit{
  display: inline-block;
  padding-bottom: 5px;
  position: relative;
}
.udrTit:before{
    content: "";
    position: absolute;
    width: 50%;
    height: 1px;
    bottom: 0;
    border-bottom: 2px solid #ffb100;
}

