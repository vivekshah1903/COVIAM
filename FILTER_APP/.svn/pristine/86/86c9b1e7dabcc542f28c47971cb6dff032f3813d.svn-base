/**/

.pst-rel {
    position: relative;
}

a .tooltiptext {
    visibility: hidden;
    width: 120px;
    background-color:rgba(0,0,0,0.8);
    color: #fff;
    text-align: center;
    /*border-radius: 6px;*/
    padding: 5px 0;
    top: 25px;
    left: 6px;
    /* Position the tooltip */
    position: absolute;
    z-index: 1;
}
a.like .tooltiptext{right:0;left:inherit;top: 30px;}
a:hover .tooltiptext {
    visibility: visible;
}
a.like .tooltiptext::after{right: 13%;left:inherit;}
a .tooltiptext::after {
    content: "";
    position: absolute;
    bottom: 100%;
    left: 14%;
    margin-left: -5px;
    border-width: 5px;
    border-style: solid;
    border-color: transparent transparent #000 transparent;
}
.glyphicon-remove, .glyphicon-ok, .glyphicon-off{margin-right: 9px;
    border: 1px solid;
    border-radius: 85%;
    padding: 10px;     
    display: inline-block;}
.glyphicon-ok{color: green;}
.glyphicon-remove{color:red;}
.glyphicon-off{color: rgb(224, 192, 27);}
