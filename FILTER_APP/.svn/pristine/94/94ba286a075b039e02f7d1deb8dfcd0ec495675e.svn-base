.nav-tabs {
    background: #ddd;
}

.nav-tabs>li {
    color: #222 !important;
}

.navbar-inverse .navbar-nav>li>a {
    cursor: pointer;
}

.nav-tabs>li.active>a,
.nav-tabs>li.active>a:hover,
.nav-tabs>li.active>a:focus {
    color: #003946;
    background: #ddd;
    font-weight: bold;
    border-bottom: 5px solid #ffb100;
}

.nav-tabs>li>a {
    margin-right: 2px;
    line-height: 1.4;
    border-radius: none !important;
  
    font-size: 16px;
    cursor: pointer;
}

.sa-innate-form input[type=text],
input[type=password],
input[type=file],
